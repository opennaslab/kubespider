{
  "name": "kubespider-extension",
  "description": "Trigger Kubespider execute downloading task",
  "version": "0.2.0",
  "manifest_version": 3,
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/icon48.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-b799e1bf.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "img/icon16.png",
        "img/icon48.png",
        "img/icon128.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/chunk-7f353fbb.js",
        "assets/chunk-fe7244a2.js",
        "assets/chunk-7e3b3ba5.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "contextMenus",
    "tabs",
    "cookies",
    "activeTab",
    "scripting"
  ]
}
